<%= render partial: "shared/header", locals: {:title => 'Listing Skill Tags'} %>
<table class="listTable">
  <thead>
  <tr>
    <th>Tag</th>
    <th>Type</th>
    <th colspan="3"></th>
  </tr>
  </thead>
  <tbody>
  <% @skill_tags.each do |skill_tag| %>
      <tr>
        <td><%= skill_tag.tag %></td>
        <td><%= skill_tag.skill_tag_type.tagType %></td>
        <td><%= link_to 'Show', admin_skill_tag_path(skill_tag) %></td>
        <td><%= link_to 'Edit', edit_admin_skill_tag_path(skill_tag) %></td>
        <td><%= link_to 'Destroy', admin_skill_tag_path(skill_tag), method: :delete, data: {confirm: 'Are you sure?'} %></td>
      </tr>
  <% end %>
  </tbody>
</table>
<%= render partial: "shared/admin_bottom_links", locals: {:links => [{:title => 'New Skill Tag', :path => new_admin_skill_tag_path}] } %>
